{
  "name": "package-json-tamer",
  "description": "Prettier formatter for package.json files",
  "license": "MIT",
  "author": "Cameron Hunter <hello@cameronhunter.co.uk>",
  "repository": {
    "type": "git",
    "url": "https://github.com/long-lazuli/package-json-tamer"
  },
  "bugs": "https://github.com/cameronhunter/package-json-tamer/issues",
  "version": "2.0.1",
  "main": "src/index.js",
  "bin": {
    "package-json-tamer": "./bin/package-json-tamer"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "\n# Sample of use:": "\r# \\n: put a new line before the line\n\\r: return to the begining of line.",
    "precommit": "package-json-tamer --write && git add package.json",
    "test": "jest",
    "preversion": "npm test",
    "version": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "commander": "^2.18.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "minimatch": "^3.0.4",
    "parse-author": "^2.0.0",
    "sort-object-keys": "^1.1.2",
    "sort-order": "^1.0.1"
  },
  "devDependencies": {
    "cross-spawn": "^6.0.5",
    "husky": "^0.14.3",
    "jest": "^23.6.0"
  },
  "keywords": [
    "cleanup",
    "formatter",
    "json",
    "keys",
    "object",
    "package",
    "package.json",
    "prettier",
    "sort"
  ],
  "engines": {
    "node": ">= 6"
  }
}
